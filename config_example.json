{
    "filetype": {
        "py": {
            "default_language": "python3",
            "run": "python3 $file"
        },
        "cpp": {
            "default_language": "c++17",
            "compile": "g++ -std=c++17 -O2 -Wall -Wno-sign-compare $file -o b.out",
            "run": "./b.out"
        }
    }
}
